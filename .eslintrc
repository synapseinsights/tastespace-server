{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
      "browser": false,
      "node": true,
      "es6": true
    },
    "rules": {
        "import/no-unresolved": [2, { "commonjs": true, "caseSensitive": false}],
        "strict": 0,
        "no-console": "off"
        
        
    },
    "plugins": [
      "import"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "modules": true
      }
    }
    
  }